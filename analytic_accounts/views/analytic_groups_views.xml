<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Type -->
	<record model="ir.ui.view" id="analytic_accounts_group_type_tree">
		<field name="name">Analytic Group - Type Tree</field>
		<field name="model">analytic_accounts.group.type</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name" />
				<field name="code" />
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="analytic_accounts_group_type_form">
		<field name="name">Analytic Group - Type Form</field>
		<field name="model">analytic_accounts.group.type</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<group>
							<field name="name" />
						</group>
						<group>
							<field name="code"/>
						</group>
						<group>
						    <field name="group_ids"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

    <!-- Group -->
	<record model="ir.ui.view" id="analytic_accounts_group_tree">
		<field name="name">Analytic Group - Group Tree</field>
		<field name="model">analytic_accounts.group</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name" />
				<field name="code" />
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="analytic_accounts_group_form">
		<field name="name">Analytic Group - Group Form</field>
		<field name="model">analytic_accounts.group</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<group>
							<field name="name" />
						</group>
						<group>
							<field name="code"/>
						</group>
						<group>
						    <field name="account_ids"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

    <!-- Account -->
	<record model="ir.ui.view" id="analytic_accounts_group_account_tree">
		<field name="name">Analytic Group - Account Tree</field>
		<field name="model">analytic_accounts.group.account</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name" />
				<field name="code" />
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="analytic_accounts_group_account_form">
		<field name="name">Analytic Group - Account Form</field>
		<field name="model">analytic_accounts.group.account</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<group>
							<field name="name" />
						</group>
						<group>
							<field name="code"/>
						</group>
						<group>
						    <field name="sub_account_ids"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

    <!-- Sub Account -->
	<record model="ir.ui.view" id="analytic_accounts_group_sub_account_tree">
		<field name="name">Analytic Group - Sub Account Tree</field>
		<field name="model">analytic_accounts.group.sub_account</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name" />
				<field name="code" />
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="analytic_accounts_group_sub_account_form">
		<field name="name">Analytic Group - Account Form</field>
		<field name="model">analytic_accounts.group.sub_account</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<group>
							<field name="name" />
						</group>
						<group>
							<field name="code"/>
						</group>
						<group>
						    <field name="item_ids"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

    <!-- Item -->
	<record model="ir.ui.view" id="analytic_accounts_group_item_tree">
		<field name="name">Analytic Group - Item Tree</field>
		<field name="model">analytic_accounts.group.item</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name" />
				<field name="code" />
			</tree>
		</field>
	</record>

	<record model="ir.ui.view" id="analytic_accounts_group_item_form">
		<field name="name">Analytic Group - Item Form</field>
		<field name="model">analytic_accounts.group.item</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<group>
							<field name="name" />
						</group>
						<group>
							<field name="code"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

</odoo>