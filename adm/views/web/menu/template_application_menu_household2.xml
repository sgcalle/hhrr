                                document.getElementById(prefix_label+'state_input').style.display='none';
                                $('.'+prefix_label+'phone-us').inputmask("999-999-9999");
                                $('.'+prefix_label+'ssn-us').inputmask("999-99-9999");
                                }
                                else {
                                document.getElementById(prefix_label+'state_select').style.display='none';
                                document.getElementById(prefix_label+'state_input').style.display='block';
                                $('.'+prefix_label+'phone-us').inputmask('remove');
                                $('.'+prefix_label+'ssn-us').inputmask('remove');
                                }
                                }
                            </script>


